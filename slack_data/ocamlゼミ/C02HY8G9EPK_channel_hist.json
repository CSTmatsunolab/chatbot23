    "blocks": [
      {
        "block_id": "plu",
        "elements": [
          {
            "elements": [
              {
                "text": "sudo apt update  ",
                "type": "text"
              },
              {
                "style": {
                  "italic": true
                },
                "text": "##インストール前のパッケージアップデート",
                "type": "text"
              },
              {
                "text": "\nsudo apt install -y bubblewrap build-essential curl git m4 unzip  ",
                "type": "text"
              },
              {
                "style": {
                  "italic": true
                },
                "text": "##OPAMのインストールに必要なライブラリを導入",
                "type": "text"
              },
              {
                "text": "\nsudo sh -c \"$(curl -sL ",
                "type": "text"
              },
              {
                "type": "link",
                "url": "https://raw.githubusercontent.com/ocaml/opam/master/shell/install.sh"
              },
              {
                "text": ")\"  ",
                "type": "text"
              },
              {
                "style": {
                  "italic": true
                },
                "text": "##スクリプトを叩いてOPAM2のインストールを行う",
                "type": "text"
              },
              {
                "text": "\nopam init  ",
                "type": "text"
              },
              {
                "style": {
                  "italic": true
                },
                "text": "##OPAM初期化",
                "type": "text"
              },
              {
                "text": "\nopam switch list-available ocaml-base-compiler  ",
                "type": "text"
              },
              {
                "style": {
                  "italic": true
                },
                "text": "##インストールできるOCamlコンパイラのバージョンを確認",
                "type": "text"
              },
              {
                "text": "\nopam switch create X.XX.X  ",
                "type": "text"
              },
              {
                "style": {
                  "italic": true
                },
                "text": "##任意のバージョンを指定",
                "type": "text"
              },
              {
                "text": "\neval $(opam env)  ",
                "type": "text"
              },
              {
                "style": {
                  "italic": true
                },
                "text": "##PATHを設定",
                "type": "text"
              }
            ],
            "type": "rich_text_section"
          }
        ],
        "type": "rich_text"
      }
    ],
